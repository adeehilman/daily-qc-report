  <Sequence DisplayName="GetCache" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:String" Name="cacheFilePath" />
      <Variable x:TypeArguments="x:String" Default="[Environment.CurrentDirectory + &quot;\Data\Cache&quot;]" Name="defaultCachePath" />
    </Sequence.Variables>
    <sap:WorkflowViewStateService.ViewState>
      <scg:Dictionary x:TypeArguments="x:String, x:Object">
        <x:Boolean x:Key="IsExpanded">True</x:Boolean>
      </scg:Dictionary>
    </sap:WorkflowViewStateService.ViewState>
    <ui:LogMessage DisplayName="Log Start GetCache" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Info" Message="[&quot;[GetCache] Starting to get cache for key: &quot; + cacheKey_In]" />
    <If Condition="[String.IsNullOrEmpty(cacheFolderPath_In)]" DisplayName="Check Cache Folder Path" sap2010:WorkflowViewState.IdRef="If_1">
      <If.Then>
        <Assign sap2010:WorkflowViewState.IdRef="Assign_1">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[cacheFilePath]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[defaultCachePath + "\" + cacheKey_In + ".txt"]</InArgument>
          </Assign.Value>
        </Assign>
      </If.Then>
      <If.Else>
        <Assign sap2010:WorkflowViewState.IdRef="Assign_2">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[cacheFilePath]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[cacheFolderPath_In + "\" + cacheKey_In + ".txt"]</InArgument>
          </Assign.Value>
        </Assign>
      </If.Else>
    </If>
    <ui:LogMessage DisplayName="Log Cache Path" sap2010:WorkflowViewState.IdRef="LogMessage_2" Level="Info" Message="[&quot;[GetCache] Cache file path: &quot; + cacheFilePath]" />
    <If Condition="[File.Exists(cacheFilePath)]" DisplayName="Check File Exists" sap2010:WorkflowViewState.IdRef="If_2">
      <If.Then>
        <Sequence DisplayName="Read Cache File" sap2010:WorkflowViewState.IdRef="Sequence_2">
          <Assign DisplayName="Read File Content" sap2010:WorkflowViewState.IdRef="Assign_3">
            <Assign.To>
              <OutArgument x:TypeArguments="x:String">[cacheValue_Out]</OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="x:String">[File.ReadAllText(cacheFilePath)]</InArgument>
            </Assign.Value>
          </Assign>
          <ui:LogMessage DisplayName="Log Cache Found" sap2010:WorkflowViewState.IdRef="LogMessage_3" Level="Info" Message="[&quot;[GetCache] Cache found for key: &quot; + cacheKey_In + &quot; with value: &quot; + cacheValue_Out]" />
        </Sequence>
      </If.Then>
      <If.Else>
        <Sequence DisplayName="Cache Not Found" sap2010:WorkflowViewState.IdRef="Sequence_3">
          <Assign DisplayName="Set Empty Value" sap2010:WorkflowViewState.IdRef="Assign_4">
            <Assign.To>
              <OutArgument x:TypeArguments="x:String">[cacheValue_Out]</OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="x:String">[""]</InArgument>
            </Assign.Value>
          </Assign>
          <ui:LogMessage DisplayName="Log Cache Not Found" sap2010:WorkflowViewState.IdRef="LogMessage_4" Level="Warn" Message="[&quot;[GetCache] Cache not found for key: &quot; + cacheKey_In]" />
        </Sequence>
      </If.Else>
    </If>
    <ui:LogMessage DisplayName="Log GetCache Complete" sap2010:WorkflowViewState.IdRef="LogMessage_5" Level="Info" Message="[&quot;[GetCache] Get cache completed for key: &quot; + cacheKey_In]" />
  </Sequence>
</Activity>
